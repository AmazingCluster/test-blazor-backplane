{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Fluxor": {
    "EnableReduxDevTools": true
  },
  "DataProtection": {
    "UseRedis": false
  },
  "AllowedHosts": "*",
  "SignalR": {
    "UseRedisBackplane": false
  },
  "Redis": {
    "host": "localhost",
    "port": 6379,
    "masterSet": "mymaster",
    "allowAdmin": true
  },
  "REDIS_PASSWORD": "MYPASSWORD"
}
